#ifndef DSP_COMMON_RATELIMIT_H_
#define DSP_COMMON_RATELIMIT_H_

namespace dsp {
namespace common {

class RateLimit {
 public:
  enum class RateLimitReturnCode : int8_t {
    OK,
    DECLINED,
  };

  RateLimit(int32_t BucketLength);
  RateLimitReturnCode 
};


}  // common
}  // dsp


#endif  // DSP_COMMON_RATELIMIT_H_
